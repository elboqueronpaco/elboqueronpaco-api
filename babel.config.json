{
    "presets": [
        [
            "@babel/preset-env", {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
      ["module-resolver",{
        "root": ["./"],
        "alias": {
          "@config": "./config",
          "@models": "./src/models",
          "@libs": "./src/libs",
          "@typeDefs": "./src/graphql/types",
          "@resolvers": "./src/graphql/resolvers"
        }
      }]
    ]
}